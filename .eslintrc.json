{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
        "no-plusplus": 0,
        "no-new": 0,
        "max-len": 0,
        "prefer-destructuring": 0,
        "consistent-return": 0,
        "class-methods-use-this": 0,
        "no-console": 0,
        "radix": 0
    },
    "env": {
        "browser": true,
        "node": true
    }
}
